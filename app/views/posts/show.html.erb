<h1>Post#<%= @post.id %></h1>
<table>
  <tr>
    <th>Title</th>
    <td><%= @post.title %></td>
  </tr>
  <tr>
    <th>Content</th>
    <td><%= @post.content %></td>
  </tr>
  <tr>
    <th>Status</th>
    <td><%= @post.status %></td>
  </tr>
  <tr>
    <th>Created at</th>
    <td><%= @post.created_at %></td>
  </tr>
</table>
